{
    "name":"statechangechecker",
    "jobs":{
        "service-scc":{
            "groups":{
                "group-scc":{
                    "containers":{
                        "scc":{
                            "args":[
                            ],
                            "command":"/home/app/src/Service/start.sh",
                            "environment":{
                                "HOSTED_PLATFORM": "APIC",
                                "WEB_HOST":"apiserver-service-cisco-statechangechecker.service.apic.local",
                                "WEB_PORT":"7443"
                            },
                            "image":"aci/statechangechecker:1.2",
                            "meta":{
                                "CPULabel":"large",
                                "MemoryLabel":"large"
                            },
                            "ports":{
                                "apiserver-port":7443
                            },
                            "services":[
                                {
                                    "name":"apiserver-service",
                                    "port":"apiserver-port"
                                }
                            ]
                        }
                    },
                    "count":1,
                    "restart":{
                        "attempts":10,
                        "delay":"25s",
                        "interval":"5m",
                        "mode":"delay"
                    }
                }
            },
            "meta":{
                "tag":"scc"
            },
            "type":"service"
        }
    }
}
